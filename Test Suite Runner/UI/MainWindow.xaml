<Window x:Class="Test_Suite_Runner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Width="600" Height="500"
        Title="Jurassic Test Suite Runner"
        FontFamily="Calibri" FontSize="16">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FFA1FFFF" Offset="0" />
            <GradientStop Color="White" Offset="0.19" />
        </LinearGradientBrush>
    </Window.Background>
    <Frame Source="StartPage.xaml" NavigationUIVisibility="Hidden" Margin="10" />
</Window>
